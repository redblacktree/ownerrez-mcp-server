{
  "name": "ownerrez-mcp-server",
  "version": "1.0.0",
  "description": "MCP server for OwnerRez API with OAuth2 authentication",
  "main": "build/index.js",
  "type": "module",
  "bin": {
    "ownerrez-mcp-server": "./build/index.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc --watch",
    "start": "node build/index.js",
    "dev": "tsc && node build/index.js",
    "dev:http": "TRANSPORT_MODE=http PORT=3000 tsc && node build/index.js",
    "oauth:url": "node get-oauth-url.mjs",
    "prepare": "npm run build"
  },
  "keywords": [
    "mcp",
    "ownerrez",
    "oauth2",
    "vacation-rental",
    "property-management"
  ],
  "author": "Dustin Rasener",
  "license": "Apache-2.0",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.0.4",
    "axios": "^1.7.9",
    "dotenv": "^16.4.7",
    "express": "^4.18.2",
    "cors": "^2.8.5"
  },
  "devDependencies": {
    "@flydotio/dockerfile": "^0.7.10",
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/node": "^22.10.5",
    "typescript": "^5.7.2"
  }
}
