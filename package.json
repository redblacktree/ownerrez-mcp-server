{
  "name": "ownerrez-mcp-server",
  "version": "1.0.0",
  "description": "MCP server for OwnerRez API with OAuth2 authentication",
  "main": "build/index.js",
  "type": "module",
  "bin": {
    "ownerrez-mcp-server": "./build/index.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc --watch",
